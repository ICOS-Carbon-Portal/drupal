<?php

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function cp_events_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
	case 'help.page.cp_events':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('CP Events Module') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function cp_events_theme() {
  $theme = [];

  return $theme;
}